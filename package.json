{
  "name": "graphitab",
  "description": "GraphiQL in a Tab",
  "private": true,
  "version": "0.1.0",
  "author": "Jason Simeone",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "compile": "tsc --noEmit",
    "format": "prettier --write .",
    "postinstall": "wxt prepare"
  },
  "dependencies": {
    "@graphiql/plugin-explorer": "^5.1.1",
    "@graphiql/toolkit": "^0.11.3",
    "graphiql": "^5.2.2",
    "qs": "^6.14.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  },
  "devDependencies": {
    "@types/qs": "^6.14.0",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "@wxt-dev/auto-icons": "^1.1.0",
    "@wxt-dev/module-react": "^1.1.5",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3",
    "wxt": "^0.20.14"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5"
  }
}
